            <stringProp name="script">import org.apache.jmeter.util.JMeterUtils

Thread.sleep(45 * 1000)

def randomNumber = (int) (Math.random() * 900000000) + 100000000;
def randomStr = String.valueOf(randomNumber)
if (!randomStr.substring(0, 3).equals(&quot;000&quot;)) {
    randomStr = &quot;1&quot; + randomStr.substring(1)
}
def value = (Integer.parseInt(randomStr.substring(0, 1)) * 10) + (Integer.parseInt(randomStr.substring(1, 2)) * 9)
    + (Integer.parseInt(randomStr.substring(2, 3)) * 8) + (Integer.parseInt(randomStr.substring(3, 4)) * 7)
    + (Integer.parseInt(randomStr.substring(4, 5)) * 6) + (Integer.parseInt(randomStr.substring(5, 6)) * 5)
    + (Integer.parseInt(randomStr.substring(6, 7)) * 4) + (Integer.parseInt(randomStr.substring(7, 8)) * 3)
    + (Integer.parseInt(randomStr.substring(8, 9)) * 2)
def checkDigit = value % 11
if (checkDigit != 0)
    checkDigit = 11 - checkDigit

def nId = String.valueOf(randomStr) + String.valueOf(checkDigit)

def mobileNo = &quot;0991&quot;+vars.get(&quot;mobile_subCode&quot;)+&quot;000&quot;
def randomGenerator = new Random()
def randNo = randomGenerator.nextInt(1000000)
mobileNo = &quot;0991&quot; + vars.get(&quot;mobile_subCode&quot;) + randNo
def tempMobile = JMeterUtils.getPropDefault(&quot;MobileNo&quot;,&quot;&quot;)
if(tempMobile == null || tempMobile == &quot;&quot;){
	//JMeterUtils.setProperty(&quot;MobileNo&quot;,mobileNo)
	tempMobile = mobileNo
}
def phone = Long.parseLong(tempMobile, 10)
phone+=1
vars.put(&quot;account_no&quot;,nId)
vars.put(&quot;nId&quot;, nId)
JMeterUtils.setProperty(&quot;MobileNo&quot;,&quot;0&quot;+phone)
//vars.put(&quot;MobileNo&quot;,&quot;0&quot;+phone)
println JMeterUtils.getPropDefault(&quot;MobileNo&quot;,&quot;&quot;)
vars.put(&quot;MobileNo&quot;,&quot;0&quot;+phone)

//println(JMeterUtils.getPropDefault(&quot;MobileNo&quot;,&quot;&quot;))</stringProp>
